msgstr "Suoritin"

#: usr/share/linuxmint/mintreport/reports/900_drivers/MintReportInfo.py:41
msgid "Unknown"
msgstr "Tuntematon"

#: usr/share/linuxmint/mintreport/reports/900_drivers/MintReportInfo.py:45
#, python-format
msgid "%s (unknown)"
msgstr "%s (tuntematon)"

